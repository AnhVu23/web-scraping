{
  "name": "web-scraping",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --ignore-watch node_modules src/index.ts",
    "start": "node build/index.js",
    "build": "tsc -p .",
    "test": "NODE_ENV=test nyc --all --reporter=text mocha --full-trace -t 10000 -b --exit -r ts-node/register src/tests/index.ts",
    "types-check": "tsc",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "format": "prettier \"**/*.+(js|json|ts|md|mdx|graphql)\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & npm run types-check"
    }
  },
  "author": "Anh Vu",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "helmet": "3.10.0",
    "lodash": "^4.17.15",
    "method-override": "2.3.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "puppeteer": "^5.2.1",
    "reflect-metadata": "0.1.12",
    "routing-controllers": "0.7.7",
    "shortid": "^2.2.15",
    "swagger-jsdoc": "^4.0.0",
    "swagger-test": "^0.8.0",
    "swagger-ui-express": "^4.1.4",
    "ts-node-dev": "1.0.0-pre.26",
    "tslint": "^6.1.3",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/body-parser": "1.16.8",
    "@types/chai": "4.1.3",
    "@types/chai-http": "3.0.3",
    "@types/express": "4.11.1",
    "@types/lodash": "4.14.92",
    "@types/mocha": "5.2.0",
    "@types/node": "9.6.6",
    "@types/swagger-jsdoc": "3.0.2",
    "@types/swagger-ui-express": "4.1.2",
    "@types/uuid": "3.4.3",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mocha": "5.1.1",
    "ts-node": "6.0.0"
  },
  "repository": {
    "type": "git"
  },
  "description": "web-scraping"
}
